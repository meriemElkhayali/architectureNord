{
    "version": 2,
    "builds": [
      {
        "src": "src/*",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/*",
        "dest": "/index.html"
      }
    ]
  }
  